.row.auto-complete-match
  img.col-xs-1.col-sm-1(src='assets/images/user.png', fallback-src='assets/images/user.png', ng-src='/api/users/{{data.id}}/profile/avatar', alt="{{ 'From' | translate }}")
  .row
    .col-xs-10.col-sm-11(ng-bind-html="$highlight(data.displayName)")
    .col-xs-10.col-sm-11(ng-bind-html="$highlight(data.email)")

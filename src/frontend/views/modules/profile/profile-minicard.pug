div
  a(href='#', title='{{user.name}} <{{user.address}}>')
    img(src='assets/images/user.png', ng-src="/api/avatars?objectType=user&email={{user.address}}", alt="{{ 'User Address' | translate }}")
    span.label(ng-class="labelclass") {{label}}
  div.ellipsis {{user.name}}
  span.ellipsis
    a(ng-href='mailto:{{user.address}}', op-inbox-compose, op-inbox-compose-users='[user]') {{user.address}}

.row.auto-complete-match
  img.col-xs-1.col-sm-1(src='images/user.png', fallback-src='images/user.png', ng-src='{{ data.avatarUrl }}', alt="{{ 'From' | translate }}")
  .row
    .col-xs-10.col-sm-11(ng-if="data.displayName" ng-bind-html="$highlight(data.displayName)")
    .col-xs-10.col-sm-11(ng-if="data.email" ng-bind-html="$highlight(data.email)")
